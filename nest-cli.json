{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
      "deleteOutDir": true,
      "plugins": ["@nestjs/swagger"]
    },
    "spec": {
      "class": false,
      "component": false,
      "module": false,
      "service": false,
      "controller": false
    }
}
